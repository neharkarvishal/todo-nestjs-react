{
  "name": "stator",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "npm run affected:lint",
    "lint:file-folder-convention": "node ./tools/enforce-file-folder-naming-convention.js",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "docker:build:webapp": "docker build -t stator-api -f dockerfiles/api/Dockerfile .",
    "docker:build:api": "docker build -t stator-webapp -f dockerfiles/webapp/Dockerfile .",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "format:prettier": "prettier --write \"./(apps|libs)/**/*.(js|ts|tsx|json|yml)\"",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postgres": "run-script-os",
    "postgres:win32": "docker-compose -f ./apps/database/postgres/docker-compose.yml up",
    "postgres:default": "sudo docker-compose -f ./apps/database/postgres/docker-compose.yml up",
    "mongo": "run-script-os",
    "mongo:win32": "docker-compose -f ./apps/database/mongo/docker-compose.yml up",
    "mongo:default": "sudo docker-compose -f ./apps/database/mongo/docker-compose.yml up",
    "rename-project": "node ./tools/rename-project.js --project-name",
    "commit": "cz"
  },
  "private": true,
