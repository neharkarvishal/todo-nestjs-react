    "mongodb": "^3.6.2",
    "nx": "^10.3.0",
    "pg": "^8.4.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "5.2.0",
    "redoc": "^2.0.0-rc.40",
